{
    "version": 2,
    "builds": [
           {
              "src": "index.js",
              "use": "@vercel/node"
            }
          ],
    "routes": [
        {
            "src": "/api/users/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/tasks/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/people/(.*)",
            "dest": "/index.js"
        }
    ]
}